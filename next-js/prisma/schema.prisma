generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model main {
  email            String   @db.VarChar
  notified_studies String[] @default([]) @db.VarChar
  username         String   @db.VarChar
  password         String   @db.VarChar
  id               BigInt   @id @default(autoincrement())
}